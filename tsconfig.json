{
  "compilerOptions": {
    "target": "es2018",
    "module": "commonjs",
    "moduleResolution": "node",
    "removeComments": true,
    "declarationMap": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "pretty": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "stripInternal": true,
    "strict": true,
    "skipLibCheck": true,
    "strictPropertyInitialization": false,
    "declaration": true,
    "sourceMap": true,
    "noEmit": false,
    "listEmittedFiles": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true
  }
}
